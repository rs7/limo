<div class="feed_row">
    <div class="feedback_grouped_row_wrap">
        <div class="feedback_grouped_row feedback_like_row">
            <table class="feedback_row_t" cellpadding="0" cellspacing="0">
                <tbody>
                    <tr>
                        <td class="feedback_row_photo">
                            <div class="feedback_row_photo"
                                 onclick="event.cancelBubble = true;">
                                <a href="{{user_link user}}" target="_blank">
                                    <div class="feedback_photo_icon"></div>
                                    <img class="feedback_row_photo" src="{{user.photo_50}}">
                                </a>
                            </div>
                        </td>
                        <td class="feedback_row_content">
                            <div class="feedback_row_group_names">
                                <a href="{{user_link user}}" target="_blank" class="mem_link">{{user_name user}}</a>
                                разонравилась ваша
                                <a href="{{photo_link photo}}" target="_blank">фотография</a>
                            </div>
                            <div class="feedback_row_group_photos clear_fix"></div>
                            <div class="feedback_row_date">
                                обнаружено:
                                <span class="rel_date rel_date_needs_update" time="{{timestamp date}}">{{date date}}</span>
                            </div>
                        </td>
                        <td class="feedback_row_photo">
                            <div class="feedback_row_photo ta_r">
                                <a href="{{photo_link photo}}" target="_blank">
                                    <img class="feedback_row_photo" src="{{photo.photo_130}}">
                                </a>
                            </div>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
